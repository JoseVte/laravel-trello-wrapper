{
    "name": "josrom/laravel-trello-wrapper",
    "description": "Trello API wrapper for Laravel 11",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^6.3 | ^7.0.1",
        "guzzlehttp/psr7": "^2.1.0",
        "illuminate/contracts": "^11.0",
        "illuminate/support": "^11.0",
        "josrom/php-trello-api": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "josrom/phpunit-unicode-printer": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelTrello\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTrello\\TrelloServiceProvider"
            ],
            "aliases": {
                "LaravelTrello": "LaravelTrello\\Facade"
            }
        }
    },
    "authors": [
        {
            "name": "Jose Vicente Orts Romero",
            "email": "jvortsromero@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
